    "repository": {
        "type": "git",
        "url": "https://github.com/pandasanjay/hacker_news"
    },
    "scripts": {
        "build:prod": "NODE_ENV=production rm -rf dist && webpack",
        "start": "webpack-dev-server",
        "dev": "rm -rf dist && webpack && npm-run-all --parallel dev:*",
        "dev:build": "webpack --watch",
        "dev:server": "nodemon --watch dist --exec \"node dist/index.js\"",
